datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id              Int      @id @default(autoincrement())
  email           String   @unique
  first_name      String
  last_name       String
  mobile          String?
  hashed_password String
  updatedAt       DateTime
  createdAt       DateTime
}
