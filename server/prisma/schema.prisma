generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  category    String?
  created_at  DateTime
  description String
  id          String   @default(dbgenerated()) @id
  image       String?
  name        String
  price       Int
  quantity    Int
  updated_aat DateTime
}

model User {
  created_at      DateTime @default(now())
  email           String   @unique
  first_name      String
  hashed_password String
  id              Int      @default(autoincrement()) @id
  last_name       String
  mobile          String?
  updated_at      DateTime
}